<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/frame_layout_style_2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <androidx.cardview.widget.CardView
        android:id="@+id/card_row_report_5"
        style="@style/card_view_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin3"
        app:cardCornerRadius="@dimen/card_view_corner"
        app:cardElevation="@dimen/card_view_elevation">


        <LinearLayout
            android:id="@+id/linlayclick"
            style="@style/linear_layout_parent_row_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_main_title_report_5"
                style="@style/oneLineBlackText_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:textSize="@dimen/txt_report_size_5"
                tools:text="fghfgh" />

            <LinearLayout
                style="@style/linear_layout_style_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:orientation="horizontal">

                <LinearLayout
                    style="@style/linear_layout_style_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:baselineAligned="false"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/eeee"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:baselineAligned="false"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <LinearLayout
                            style="@style/linear_layout_style_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatImageView
                                style="@style/img_style0"
                                android:layout_width="@dimen/half_toolbar_height"
                                android:layout_height="@dimen/half_toolbar_height"
                                android:contentDescription="@string/attach_albom"
                                app:srcCompat="@drawable/ic_calendar" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/txt_sub_titile_report_5"
                                style="@style/oneLineGrayText_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="@dimen/txt_report_size_1"
                                tools:text="dfgdfgdfs" />


                        </LinearLayout>

                        <LinearLayout
                            style="@style/linear_layout_style_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatImageView
                                style="@style/img_style0"
                                android:layout_width="@dimen/half_toolbar_height"
                                android:layout_height="@dimen/half_toolbar_height"
                                android:contentDescription="@string/attach_albom"
                                app:srcCompat="@drawable/ic_distributer" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/txt_sub_titile_2_report_5"
                                style="@style/oneLineGrayText_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="@dimen/txt_report_size_1"
                                tools:text="dfgdfgdfs" />


                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/sdfsdfsdfdf"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:baselineAligned="false"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <LinearLayout
                            style="@style/linear_layout_style_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatImageView
                                style="@style/img_style"
                                android:layout_width="@dimen/half_toolbar_height"
                                android:layout_height="@dimen/half_toolbar_height"
                                android:contentDescription="@string/attach_albom"
                                app:srcCompat="@drawable/ic_fuel_level" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/txt_sub_titile_3_report_5"
                                style="@style/oneLineGrayText_style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="@dimen/txt_report_size_1"
                                tools:text="dfgdff" />

                        </LinearLayout>

                        <LinearLayout
                            style="@style/linear_layout_style_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatImageView
                                style="@style/img_style0"
                                android:layout_width="@dimen/half_toolbar_height"
                                android:layout_height="@dimen/half_toolbar_height"
                                android:contentDescription="@string/attach_albom"
                                app:srcCompat="@drawable/ic_login" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/txt_sub_titile_4_report_5"
                                style="@style/forLine_blackText_style_value_min_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="?attr/colorGrayAttr"
                                android:textSize="@dimen/txt_report_size_1"
                                tools:text="" />

                        </LinearLayout>
                    </LinearLayout>


                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_sub_titile_5_report_5"
                        style="@style/max_20_minOne_Line_blackText_style_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/linlay_margin"
                        android:gravity="start"
                        android:textColor="?attr/colorGrayAttr"
                        android:textSize="@dimen/txt_report_size_1"
                        tools:text="ertertert" />


                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/done_work_image_report_5"
        style="@style/img_style0"
        android:layout_width="@dimen/half_toolbar_parent_height_with_margin"
        android:layout_height="@dimen/half_toolbar_parent_height_with_margin"
        android:layout_gravity="top|end"
        android:layout_margin="@dimen/margin1"
        android:contentDescription="@string/attach_albom"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_chat_room_send_green"
        tools:visibility="visible" />

</FrameLayout>






